<main class="main">
  <% @articles.each do |article| %>
    <ul class="list-group">
      <li class="list-group-item">
        <p>
          <%= link_to(article.title, article_path(article))%>
        </p>
        <p><%= article.updated_at %></p>
        <ul>
          <li>
            <a href="#"><i class="fas fa-edit"></i></a>
          </li>
          <li>
            <a href="#"><i class="fas fa-trash"></i></a>
          </li>
        </ul>
      </li>
    </ul>
  <% end %>
</main> 


  